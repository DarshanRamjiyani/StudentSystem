    <!-- General JS Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.nicescroll/3.7.6/jquery.nicescroll.min.js"></script>
    <script src="/assets/js/scripts.js"></script>
    <script src="/assets/js/stisla.js"></script>
    <script src="/assets/js/insertHelpers.js"></script>
    <script>
        $('#uploadedFile1').change(function () {
            var i = $(this).prev('label').clone();
            var file = $('#uploadedFile1')[0].files[0].name;
            $('#uploadedFileLabel1').text(file);
        });
        $('#uploadedFile2').change(function () {
            var i = $(this).prev('label').clone();
            var file = $('#uploadedFile2')[0].files[0].name;
            $('#uploadedFileLabel2').text(file);
        });


    </script>
</body>

