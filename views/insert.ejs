            <!-- Main Content -->
            <div class="main-content">
                <section class="section">
                    <div class="section-header">
                        <h1>Insert student</h1>
                    </div>

                    <div class="section-body">
                        <h2 class="section-title">
                            Insert a single student by filling the form below.
                        </h2>
                        <div class="row">
                            <div class="col" >
                                <div class="card">
                                    <div class="card-header">
                                        <h4>Singular Insert</h4>
                                    </div>

                                    <div class="card-body">
                                        <form action="/student/register/initiate" method="get" id="documentForm">
                                            <!-- <div class="form-group">
                                                <label>Image</label>
                                                <div class="custom-file">
                                                    <input type="file" accept=".png,.jpeg,.jpg" class="custom-file-input" id="uploadedFile1" name="uploadedFile">
                                                    <label class="custom-file-label" for="userImage" id="uploadedFileLabel1">Choose
                                                        file</label>
                                                </div>
                                            </div> -->
                                            <div class="form-group">
                                                <label>First name</label>
                                                <input type="text" name="firstName" id="firstName"
                                                    class="form-control form-control-sm" />
                                            </div>
                                            <div class="form-group">
                                                <label>Last name</label>
                                                <input type="text" name="lastName" id="lastName"
                                                    class="form-control form-control-sm" />
                                            </div>
                                            <div class="form-group">
                                                <label>Age</label>
                                                <select class="form-control form-control-sm" name="age" id="age">
                                                    <% for(let i=5; i<=18; i++){ %>
                                                        <option value="<%= i %>">
                                                            <%= i %> Years
                                                        </option>
                                                        <% } %>
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <label>Subject</label>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" name="subjects" id="subjects"
                                                        value="C Language" />
                                                    <label class="form-check-label" for="defaultCheck2">
                                                        C Language
                                                    </label>
                                                </div>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" name="subjects"
                                                        value="Python" id="chkPython" />
                                                    <label class="form-check-label" for="defaultCheck2">
                                                        Python
                                                    </label>
                                                </div>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" name="subjects"
                                                        value="Node" id="chkNode" />
                                                    <label class="form-check-label" for="defaultCheck2">
                                                        Node
                                                    </label>
                                                </div>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" name="subjects"
                                                        value="Sanskrit" id="chkSanskrit" />
                                                    <label class="form-check-label" for="defaultCheck2">
                                                        Sanskrit
                                                    </label>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label>Average Score</label>
                                                <input type="text" name="avgScore"
                                                    class="form-control form-control-sm" />
                                            </div>

                                            <div class="form-group">
                                                <label>Grade</label>
                                                <select class="form-control" name="grade">
                                                    <option value="A">A</option>
                                                    <option value="B">B</option>
                                                    <option value="C">C</option>
                                                    <option value="D">D</option>
                                                </select>
                                            </div>

                                            <div class="form-group">
                                                <input type="submit" class="btn btn-primary" name="action"
                                                    value="Submit Data" />
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!-- <div class="col-12 col-md-6 col-lg-6">
                                <div class="card">
                                    <div class="card-header">
